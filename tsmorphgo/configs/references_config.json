{
  "cacheSettings": {
    "enabled": true,
    "maxEntries": 1000,
    "ttl": "10m",
    "cleanupInterval": "5m",
    "enableFileHashCheck": true
  },
  "retrySettings": {
    "enabled": true,
    "maxRetries": 3,
    "baseDelay": "100ms",
    "maxDelay": "5s",
    "backoffFactor": 2.0,
    "retryableErrors": [
      "timeout",
      "unavailable",
      "service",
      "connection",
      "network"
    ]
  },
  "performanceSettings": {
    "enableMetrics": true,
    "metricsInterval": "1m",
    "enableBatching": true,
    "batchSize": 50,
    "timeout": "30s"
  },
  "fallbackSettings": {
    "enabled": true,
    "enableContextAnalysis": true,
    "fallbackTimeout": "2s",
    "maxFallbackResults": 100
  },
  "loggingSettings": {
    "enabled": true,
    "level": "info",
    "includeTimestamp": true,
    "logCacheOperations": true,
    "logRetryAttempts": true,
    "logFallbackUsage": true
  }
}